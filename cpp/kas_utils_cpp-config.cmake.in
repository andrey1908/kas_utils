@PACKAGE_INIT@

set(kas_utils_cpp_INCLUDE_DIRS "@PACKAGE_INCLUDE_DIRS@")

find_library(kas_utils_cpp_metrics_LIBRARY metrics
    REQUIRED
    PATHS "@PACKAGE_LIBRARY_DIRS@"
    NO_DEFAULT_PATH
)
find_library(kas_utils_cpp_depth_to_point_cloud_LIBRARY depth_to_point_cloud
    REQUIRED
    PATHS "@PACKAGE_LIBRARY_DIRS@"
    NO_DEFAULT_PATH
)
find_library(
    kas_utils_cpp_dilation_LIBRARY dilation
    REQUIRED
    PATHS "@PACKAGE_LIBRARY_DIRS@"
    NO_DEFAULT_PATH
)
list(APPEND kas_utils_cpp_LIBRARIES
    ${kas_utils_cpp_metrics_LIBRARY}
    ${kas_utils_cpp_depth_to_point_cloud_LIBRARY}
    ${kas_utils_cpp_dilation_LIBRARY}
)

set(kas_utils_cpp_LIBRARIES_DIR "@PACKAGE_LIBRARY_DIRS@")
get_filename_component(
    kas_utils_cpp_LIBRARIES_DIR
    ${kas_utils_cpp_LIBRARIES_DIR} REALPATH)

file(GLOB kas_utils_cpp_depth_to_point_cloud_PYTHON_LIBRARY
    "@PACKAGE_LIBRARY_DIRS@/python3/dist-packages/py_depth_to_point_cloud.*.so"
)
if (kas_utils_cpp_depth_to_point_cloud_PYTHON_LIBRARY)
    get_filename_component(
        kas_utils_cpp_depth_to_point_cloud_PYTHON_LIBRARY
        ${kas_utils_cpp_depth_to_point_cloud_PYTHON_LIBRARY} REALPATH)
endif()
list(APPEND kas_utils_cpp_PYTHON_LIBRARIES
    ${kas_utils_cpp_depth_to_point_cloud_PYTHON_LIBRARY})
